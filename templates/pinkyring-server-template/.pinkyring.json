{
  "templateName": "pinkyring-server-template",
  "pinkyringVersion": "0.1.0",
  "removableOptions": [
    {
      "label": "SERVERLESS",
      "globPatterns": [
        ".github/workflows/serverless.*",
        "packages/infrastructure/aws",
        "serverless",
        "serverless.yml"
      ],
      "contentPattern": ".pinkyring=SERVERLESS",
      "typescriptReferences": ["infrastructure/aws/lambdas", "infrastructure/aws/snqs"],
      "packageNames": ["@test/infrastructure_aws_lambdas", "@test/infrastructure_aws_snqs"]
    },
    {
      "label": "GITHUB WORKFLOWS",
      "globPatterns": [".github"]
    },
    {
      "label": "REST ENDPOINTS",
      "globPatterns": ["packages/apps/api-rest"]
    }
  ]
}
