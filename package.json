{
  "name": "pinkyring",
  "version": "1.0.0",
  "private": true,
  "description": "A node project generator that allows you to try a project template, and then remove the pieces you don't want.",
  "keywords": [
    "pinkyring",
    "project generator",
    "templates"
  ],
  "homepage": "https://github.com/CaseyHaralson/pinkyring#readme",
  "bugs": {
    "url": "https://github.com/CaseyHaralson/pinkyring/issues"
  },
  "license": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaseyHaralson/pinkyring.git"
  },
  "author": "Casey Haralson",
  "bin": {
    "pinkyring": "./entry-file.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "npx rimraf ./dist && tsc -b",
    "prettier": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "lint": "npx eslint . --report-unused-disable-directives --max-warnings=0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "eslint": "^8.26.0",
    "prettier": "2.7.1",
    "typescript": "^4.9.3"
  }
}
